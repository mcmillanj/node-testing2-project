  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.14",
    "sqlite3": "^5.0.2"
  }
